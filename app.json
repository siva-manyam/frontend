{
  "expo": {
    "name": "Family Safety Tracker",
    "slug": "family-safety-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "family-safety-tracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.familysafety.tracker",
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location for family safety tracking.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location for family safety tracking.",
        "NSContactsUsageDescription": "This app needs access to your contacts to sync with parent device.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to sync metadata with parent device.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "package": "com.familysafety.tracker",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#007AFF"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "READ_CONTACTS",
        "READ_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "ACCESS_MEDIA_LOCATION"
      ],
      "edgeToEdgeEnabled": true,
      "runtimeVersion": "1.0.0",
      "statusBar": {
        "backgroundColor": "#007AFF",
        "barStyle": "light-content"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ad9c3ef8-e541-4256-8322-2518cdab5bb4"
      }
    },
    "owner": "hacker1921",
    "updates": {
      "url": "https://u.expo.dev/ad9c3ef8-e541-4256-8322-2518cdab5bb4"
    }
  }
}